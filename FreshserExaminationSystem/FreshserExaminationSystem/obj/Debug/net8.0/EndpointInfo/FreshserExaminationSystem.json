            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "durationInMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "totalMarks": {
            "type": "integer",
            "format": "int32"
          },
          "passPercentage": {
            "type": "number",
            "format": "double"
          },
          "isPublished": {
            "type": "boolean"
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "questions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Question"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Login": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Question": {
        "type": "object",
        "properties": {
          "examId": {
            "type": "integer",
            "format": "int32"
          },
          "questionText": {
            "type": "string",
            "nullable": true
          },
          "isMultipleChoice": {
            "type": "boolean"
          },
          "optionA": {
            "type": "string",
            "nullable": true
          },
          "optionB": {
            "type": "string",
            "nullable": true
          },
          "optionC": {
            "type": "string",
            "nullable": true
          },
          "optionD": {
            "type": "string",
            "nullable": true
          },
          "correctOption": {
            "type": "string",
            "nullable": true
          },
          "marks": {
            "type": "integer",
            "format": "int32"
          },
          "exam": {
            "$ref": "#/components/schemas/Exam"
          }
        },
        "additionalProperties": false
      },
      "SubmitAllDto": {
        "type": "object",
        "properties": {
          "attemptId": {
            "type": "integer",
            "format": "int32"
          },
          "answers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AnswerDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "User": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "password": {
            "maxLength": 10,
            "type": "string",
            "nullable": true
          },
          "isAdmin": {
            "type": "boolean"
          },
          "role": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          }
        },
        "additionalProperties": false
      }
    },
